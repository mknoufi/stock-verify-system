{
  "include": [
    "**/*.py"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/.venv",
    "**/venv",
    "**/.env"
  ],
  "reportMissingImports": "warning",
  "reportMissingModuleSource": "warning",
  "pythonVersion": "3.10",
  "pythonPlatform": "All",
  "typeCheckingMode": "strict",
  "venvPath": ".",
  "venv": ".venv",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.10",
      "pythonPlatform": "All",
      "extraPaths": [
        ".",
        "backend"
      ]
    }
  ]
}
