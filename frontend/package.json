{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "overrides": {
    "react-test-renderer": "19.1.0"
  },
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "postinstall": "node -e \"if (process.env.SKIP_POSTINSTALL === 'true') { process.exit(0); }\" && patch-package",
    "prestart": "npm run update-ip --silent",
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "start:offline": "expo start --offline",
    "start:clear": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "update-ip": "node ./scripts/update-ip.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:clear": "expo start --web --clear",
    "lint": "expo lint",
    "lint:fix": "expo lint -- --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "build:web": "expo export -p web",
    "build:android": "eas build -p android",
    "build:ios": "eas build -p ios",
    "ci": "npm run lint && npm run typecheck && npm run test",
    "ci:parallel": "npm-run-all --parallel lint typecheck test",
    "preflight": "npm-run-all lint typecheck test build:web",
    "validate": "npm-run-all --parallel lint typecheck && npm run test",
    "clean": "rm -rf node_modules/.cache .expo dist",
    "clean:all": "rm -rf node_modules .expo dist && npm install",
    "deps:check": "npx npm-check -u",
    "deps:outdated": "npm outdated",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "cd .. && husky install frontend/.husky || true"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^5.59.16",
    "axios": "^1.7.9",
    "expo": "~54.0.29",
    "expo-background-fetch": "~14.0.9",
    "expo-blur": "~15.0.7",
    "expo-brightness": "~14.0.8",
    "expo-camera": "~17.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.8",
    "expo-file-system": "~19.0.18",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-keep-awake": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-notifications": "~0.32.15",
    "expo-router": "~6.0.19",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.7",
    "expo-speech": "~14.0.8",
    "expo-splash-screen": "~31.0.11",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~14.0.9",
    "lottie-react-native": "^7.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.68.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "use-debounce": "^10.0.6",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@expo/cli": "^54.0.0",
    "@expo/metro-runtime": "~6.1.2",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "babel-jest": "~29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-expo": "~54.0.16",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.1",
    "playwright": "^1.57.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.7.4",
    "storybook": "^8.6.15",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
